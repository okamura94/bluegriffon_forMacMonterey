<?xml version="1.0"?>
  
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>scripteditor@bluegriffon.com</em:id>
    <em:version>2.0</em:version>
  
    <em:targetApplication>
      <!-- BlueGriffon -->
      <Description>
        <em:id>bluegriffon@bluegriffon.com</em:id>
        <em:minVersion>3.2</em:minVersion>
        <em:maxVersion>*</em:maxVersion>
      </Description>
    </em:targetApplication>
  
    <!-- front-end metadata -->
    <em:name>Script Inspector</em:name>
    <em:description>Script Inspector for BlueGriffon</em:description>
    <em:creator>Disruptive Innovations SARL</em:creator>
    <em:homepageURL>http://www.disruptive-innovations.com/</em:homepageURL>
    <em:iconURL>chrome://scripteditor/skin/scripteditor-icon.png</em:iconURL>

    <em:file>
      <Description about="scripteditor.jar">
        <em:package>content/</em:package>
        <em:locale>locale/en-US/</em:locale>
      </Description>
    </em:file>

  </Description>
</RDF>
